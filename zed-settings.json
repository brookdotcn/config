{
  "tab_size": 2,
  "ui_font_size": 13,
  "ui_font_weight": 300,
  "buffer_font_size": 16,
  "base_keymap": "VSCode",
  "buffer_font_weight": 300,
  "autosave": "on_focus_change",
  "ui_font_family": "Geist Mono",
  "buffer_font_family": "Geist Mono",

  "scrollbar": { "show": "never" },
  "terminal": { "button": false },
  "chat_panel": { "button": false },
  "outline_panel": { "button": false },
  "project_panel": { "button": false },
  "notification_panel": { "button": false },
  "collaboration_panel": { "button": false },
  "tab_bar": { "show_nav_history_buttons": false },
  "toolbar": { "breadcrumbs": false, "quick_actions": false },

  "assistant": {
    "version": "2",
    "default_model": { "provider": "ollama", "model": "llama3.1:latest" }
  },

  "experimental.theme_overrides": {
    "text": "#999999",
    "error": "#f97583",
    "border": "#2A2A2A",
    "hidden": "#727272",
    "hint": "#758575dd",
    "created": "#6C9C77",
    "deleted": "#f97583",
    "modified": "#AB9775",
    "ignored": "#99999980",
    "text.muted": "#999999",
    "predictive": "#999999",
    "text.accent": "#999999",
    "error.border": "#f97583",
    "background": "#1A1A1Ae0",
    "element.hover": "#2A2A2A",
    "border.variant": "#1F1F1F",
    "editor.foreground": "#BBB",
    "link_text.hover": "#BBBBBB",
    "element.selected": "#2A2A2A",
    "element.active": "#4d937525",
    "error.background": "#1A1A1A",
    "editor.background": "#1A1A1A",
    "terminal.ansi.red": "#FA7584",
    "surface.background": "#2A2A2A",
    "toolbar.background": "#1A1A1A",
    "tab_bar.background": "#1F1F1F",
    "terminal.ansi.blue": "#B392F1",
    "terminal.ansi.cyan": "#79B8FF",
    "deleted.background": "#1A1A1A",
    "panel.background": "#00000000",
    "editor.line_number": "#727272",
    "terminal.ansi.black": "#1A1A1A",
    "terminal.background": "#1A1A1A",
    "terminal.foreground": "#BBBBBB",
    "terminal.ansi.white": "#BBBBBB",
    "terminal.ansi.green": "#79B8FF",
    "element.background": "#4d937550",
    "title_bar.background": "#1F1F1F",
    "terminal.ansi.yellow": "#FFAB70",
    "status_bar.background": "#1F1F1F",
    "tab.active_background": "#1A1A1A",
    "background.appearance": "blurred",
    "terminal.ansi.dim_red": "#D15A68",
    "terminal.ansi.magenta": "#E394DC",
    "terminal.ansi.dim_blue": "#8A6EC0",
    "ghost_element.selected": "#1F1F1F",
    "terminal.ansi.dim_cyan": "#5A8AC0",
    "terminal.ansi.dim_white": "#898989",
    "terminal.dim_foreground": "#898989",
    "terminal.ansi.dim_black": "#000000",
    "tab.inactive_background": "#1A1A1A",
    "terminal.ansi.dim_green": "#5A8AC0",
    "drop_target.background": "#2A2A2A50",
    "terminal.ansi.bright_red": "#FF9CAA",
    "terminal.ansi.dim_yellow": "#D18F5A",
    "editor.gutter.background": "#1A1A1A",
    "terminal.ansi.dim_magenta": "#B376B0",
    "search.match_background": "#FFAB7064",
    "terminal.ansi.bright_blue": "#CFB8FF",
    "terminal.ansi.bright_cyan": "#A3D0FF",
    "editor.active_line_number": "#727272",
    "terminal.bright_foreground": "#F8F8F8",
    "terminal.ansi.bright_white": "#F8F8F8",
    "terminal.ansi.bright_green": "#A3D0FF",
    "terminal.ansi.bright_black": "#5C5C5C",
    "terminal.ansi.bright_yellow": "#FFC9A3",
    "elevated_surface.background": "#2A2A2A",
    "editor.subheader.background": "#2A2A2A",
    "terminal.ansi.bright_magenta": "#F3B8EC",
    "editor.active_line.background": "#292929",

    "syntax": {
      "tag": { "color": "#4d9375" },
      "type": { "color": "#5DA994" },
      "label": { "color": "#80a665" },
      "title": { "color": "#80a665" },
      "string": { "color": "#c98a7d" },
      "number": { "color": "#4C9A91" },
      "keyword": { "color": "#4d9375" },
      "boolean": { "color": "#4d9375" },
      "function": { "color": "#80a665" },
      "constant": { "color": "#c99076" },
      "operator": { "color": "#cb7676" },
      "link_uri": { "color": "#c98a7d" },
      "variable": { "color": "#bd976a" },
      "comment": { "color": "#758575dd" },
      "attribute": { "color": "#bd976a" },
      "link_text": { "color": "#c98a7d" },
      "property": { "color": "#dbd7caee" },
      "constructor": { "color": "#4d9375" },
      "punctuation": { "color": "#666666" },
      "string.regex": { "color": "#c98a7d" },
      "text.literal": { "color": "#c98a7d" },
      "comment.doc": { "color": "#758575dd" },
      "string.escape": { "color": "#c98a7d" },
      "string.special": { "color": "#c98a7d" },
      "variable.special": { "color": "#c99076" },
      "punctuation.special": { "color": "#666666" },
      "punctuation.bracket": { "color": "#666666" },
      "string.special.symbol": { "color": "#c98a7d" },
      "punctuation.delimiter": { "color": "#666666" },
      "punctuation.list_marker": { "color": "#666666" },
      "emphasis": { "color": "#dbd7caee", "font_style": "italic" },
      "emphasis.strong": { "color": "#dbd7caee", "font_weight": 700 }
    }
  }
}
